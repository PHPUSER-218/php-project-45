{
  "name": "hexlet/code",
  "description": "This is a simple PHP package for brain games",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Hexlet\\Code\\": "src/"
    },
    "files": [
      "src/Cli.php",
      "src/Engine.php",
      "src/Games/Even.php",
      "src/Games/Calc.php",
      "src/Games/Progression.php",
      "src/Games/Prime.php"
    ],
    "classmap": [],
    "psr-0": {},
    "exclude-from-classmap": []
  },
  "bin": [
    "bin/brain-games",
    "bin/brain-even",
    "bin/brain-calc",
    "bin/brain-gcd",
    "bin/brain-progression",
    "bin/brain-prime"
  ],
  "authors": [
    {
      "name": "Victoria Karpovich",
      "email": "akvio2019@gmail.com"
    }
  ],
  "require": {
    "wp-cli/php-cli-tools": "^0.12.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.11"
  }
}
